{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".tubo-out/**", "lib/**"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "test:watch": {
      "dependsOn": ["build"]
    },
    "test:coverage": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**"]
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "docs": {},
    "clean": {
      "cache": false
    }
  }
}
